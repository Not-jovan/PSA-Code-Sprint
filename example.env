# .env.example
# Copy this file to `.env` and fill in your real values.
# Never commit your actual `.env` file to version control.


# OpenAI API key for chatbot features

AZURE_OPENAI_API_KEY=yourkeyhere
AZURE_OPENAI_BASE_URL=https://psacodesprint2025.azure-api.net/openai
AZURE_OPENAI_CHAT_MODEL=gpt-5-mini
AZURE_OPENAI_API_VERSION=2025-01-01-preview
PORT=3000


# Optional: CRA-specific runtime configuration
# React recognizes any variable prefixed with REACT_APP_
REACT_APP_ENV=development
REACT_APP_API_BASE_URL=http://localhost:8080

# Example toggle for dark mode default
REACT_APP_DEFAULT_THEME=dark
